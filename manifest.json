{
  "manifest_version": 3,
  "name": "Video Finder",
  "version": "1.0",
  "description": "Find and download HTML5 videos on the page.",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
